@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="https://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<div class="container-fluid">    
    <a href="~/Home/Formulario" class="btn btn-primary btn-lg active" role="button" aria-pressed="true">Formulario</a>
</div>

<div class="container">
    <div class="row">
        <!--Datos del Req.-->
        <div class="col-lg-4">
            <form class="form-horizontal" enctype="multipart/form-data">
                <div class="form-group">
                    <label class="col-lg-offset-2 control-label">Datos de Requerimiento</label>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">ID Requerimiento:</label>
                    <div class="col-lg-6">
                        <input type="text" name="id" class="form-control" placeholder="Id" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Gerente Comercial:</label>
                    <div class="col-lg-6">
                        <input type="text" name="Tipo_usuario" class="form-control" placeholder="Tipo Usuario" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Motivo Req.:</label>
                    <div class="col-lg-6">
                        <input type="text" name="E_mail" class="form-control" placeholder="E-mail" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Cliente:</label>
                    <div class="col-lg-6">
                        <input type="password" name="Password" class="form-control" placeholder="Password" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Área o Gerencia:</label>
                    <div class="col-lg-6">
                        <input type="password" name="Password" class="form-control" placeholder="Password" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Empresa Cliente:</label>
                    <div class="col-lg-6">
                        <input type="password" name="Password" class="form-control" placeholder="Password" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Email Solicitante:</label>
                    <div class="col-lg-6">
                        <input type="password" name="Password" class="form-control" placeholder="Password" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Tel. Solicitante:</label>
                    <div class="col-lg-6">
                        <input type="password" name="Password" class="form-control" placeholder="Password" />
                    </div>
                </div>
            </form>
        </div>
        <!--Perfil-->
        <div class="col-lg-4">
            <form class="form-horizontal" enctype="multipart/form-data">
                <div class="form-group">
                    <label class="col-lg-6 control-label">Perfil</label>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Perfil Solicitado:</label>
                    <div class="col-lg-6">
                        <input type="text" name="id" class="form-control" placeholder="Id" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Carrera Solicitante:</label>
                    <div class="col-lg-6">
                        <input type="text" name="Tipo_usuario" class="form-control" placeholder="Tipo Usuario" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Nivel Estudios:</label>
                    <div class="col-lg-6">
                        <input type="text" name="E_mail" class="form-control" placeholder="E-mail" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Años Experiencia:</label>
                    <div class="col-lg-6">
                        <input type="password" name="Password" class="form-control" placeholder="Password" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Cantidad Recursos:</label>
                    <div class="col-lg-6">
                        <input type="password" name="Password" class="form-control" placeholder="Password" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Áreas Carreras:</label>
                    <div class="col-lg-6">
                        <input type="password" name="Password" class="form-control" placeholder="Password" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Certificaciones:</label>
                    <div class="col-lg-6">
                        <input type="password" name="Password" class="form-control" placeholder="Password" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Prioridad Req.:</label>
                    <div class="col-lg-6">
                        <input type="password" name="Password" class="form-control" placeholder="Password" />
                    </div>
                </div>
            </form>
        </div>
        <!--Fecha Req.-->
        <div class="col-lg-4">
            <form class="form-horizontal" enctype="multipart/form-data">
                <div class="form-group">
                    <label class="col-lg-6 control-label">Fechas Requerimientos</label>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Fecha Solicitud:</label>
                    <div class="col-lg-6">
                        <input type="text" name="id" class="form-control" placeholder="Id" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Fecha Envio:</label>
                    <div class="col-lg-6">
                        <input type="text" name="id" class="form-control" placeholder="Id" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Fecha Cierre:</label>
                    <div class="col-lg-6">
                        <input type="text" name="id" class="form-control" placeholder="Id" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label"></label>
                    <div class="col-lg-6">
                        <label class="col-lg-6 control-label"></label>
                    </div>
                </div>
            </form>
            <hr />
            <form class="form-horizontal" enctype="multipart/form-data">
                <div class="form-group">
                    <label class="col-lg-6 control-label">General</label>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Rango de Edad:</label>
                    <div class="col-lg-6">
                        <input type="text" name="id" class="form-control" placeholder="Id" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Sexo:</label>
                    <div class="col-lg-6">
                        <input type="text" name="id" class="form-control" placeholder="Id" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Idiomas:</label>
                    <div class="col-lg-6">
                        <input type="text" name="id" class="form-control" placeholder="Id" />
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>
<hr />
<div class="container">
    <div class="row">
        <!--Economia-->
        <div class="col-lg-4">
            <form class="form-horizontal" enctype="multipart/form-data">
                <div class="form-group">
                    <label class="col-lg-offset-2 control-label">Economia</label>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Rango de Tarifa:</label>
                    <div class="col-lg-6">
                        <input type="text" name="id" class="form-control" placeholder="Id" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Rango de Sueldos:</label>
                    <div class="col-lg-6">
                        <input type="text" name="Tipo_usuario" class="form-control" placeholder="Tipo Usuario" />
                    </div>
                </div>
            </form>
        </div>
        <!--Adicionales-->
        <div class="col-lg-4">
            <div>
                <div class="form-group">
                    <label class="control-label">Adicionales</label>
                </div>
                <div class="form-group">
                    <textarea class="form-control" rows="4"></textarea>
                </div>
            </div>
        </div>
        <!--Publicacion-->
        <div class="col-lg-4">
            <form class="form-horizontal" enctype="multipart/form-data">
                <div class="form-group">
                    <label class="col-lg-6 control-label">Publicacion</label>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Compu Trabajo:</label>
                    <div class="col-lg-6">
                        <input type="text" name="id" class="form-control" placeholder="Id" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Redes Sociales:</label>
                    <div class="col-lg-6">
                        <input type="text" name="id" class="form-control" placeholder="Id" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">Paginas Web:</label>
                    <div class="col-lg-6">
                        <input type="text" name="id" class="form-control" placeholder="Id" />
                    </div>
                </div>
                <div class="form-group">
                    <label class="col-lg-6 control-label">OCC Mundial:</label>
                    <div class="col-lg-6">
                        <input type="text" name="id" class="form-control" placeholder="Id" />
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>